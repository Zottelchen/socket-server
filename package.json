    "axios": "^1.4.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.2",
    "ejs": "^3.1.9",
    "express": "^4.17.1",
    "mongoose": "^5.9.25",
    "morgan": "^1.10.0",
    "request": "^2.88.2",
    "socket.io": "^2.3.0",
    "uuidv4": "^6.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "jasmine": "^3.6.1",
    "mocha": "^10.2.0",
    "postcss": "^8.4.26",
    "socket.io-client": "^2.3.0",
    "tailwindcss": "^3.3.3"
